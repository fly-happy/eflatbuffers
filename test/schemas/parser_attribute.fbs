attribute "priority";
attribute "name";

enum Color : byte { Red, Green, Blue }
union Animal (priority: 4) { Dog, Cat }

table Dog {
  age:int;
}

table Cat {
  lives:int;
}

struct Nest (priority: 3) {
  shortNum:short;
}

table State (priority: 2, name: "Foo:Bar") {
  active:bool = false (deprecated, priority: 1);
  color:Color (priority: 3);
  animal:Animal;
  nest:Nest;
}

root_type State;